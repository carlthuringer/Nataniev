#!/bin/env ruby
# encoding: utf-8

# You see nothing, enter the nothing.

require_relative "nataniev.rb"

$nataniev = Nataniev.new
$nataniev.start
minute = Time.now.strftime("%M").to_i

# Bots
if minute == 16 then $nataniev.make_anonym("disms").actions.tweet_auto end
if minute == 26 then $nataniev.make_anonym("willw").actions.tweet_auto end

puts $nataniev.make_anonym("willw").actions.tweet_reply_auto